        <dependency org="org.junit.vintage"      name="junit-vintage-engine"    rev="5.10.2"/>
        <dependency org="junit"                  name="junit"                   rev="4.13.2"/>
        <dependency org="org.apache.derby"       name="derby"                   rev="10.15.2.0"/>
        <dependency org="org.apache.derby"       name="derbytools"              rev="10.15.2.0"/>
        <dependency org="org.apache.derby"       name="derbyshared"             rev="10.15.2.0"/>
        <dependency org="org.hsqldb"             name="hsqldb"                  rev="2.7.2"/>
        <dependency org="com.h2database"         name="h2"                      rev="2.2.224"/>
        <dependency org="gov.nist.math"          name="jama"                    rev="1.0.3"/>
